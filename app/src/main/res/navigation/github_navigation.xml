<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/splash">

    <fragment
            android:id="@+id/splash"
            android:name="liuuu.laurence.foundationprojectmvvmandroid.SplashFragment"
            android:label="@string/splash">
        <action android:id="@+id/action_splash_to_gitHubFragment" app:destination="@id/github"
                app:enterAnim="@anim/nav_default_enter_anim" app:exitAnim="@anim/nav_default_exit_anim"/>
    </fragment>
    <fragment android:id="@+id/github" android:name="liuuu.laurence.foundationprojectmvvmandroid.GitHubFragment"
              android:label="@string/github"/>
</navigation>